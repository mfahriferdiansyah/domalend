{
	"name": "domalend-avs",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"start:operator": "ts-node operator/index.ts",
		"start:traffic": "ts-node operator/createNewTasks.ts",
		"start:anvil": "anvil",
		"deploy:core": "cd contracts && make deploy-core network=domaTestnet",
		"deploy:domalend": "cd contracts && make deploy-domalend network=domaTestnet",
		"deploy:core-local": "cd contracts && forge script script/DeployEigenLayerCore.s.sol --rpc-url http://localhost:8545 --broadcast",
		"deploy:domalend-local": "cd contracts && forge script script/ContractDeployer.s.sol --rpc-url http://localhost:8545 --broadcast",
		"build": "cd contracts && forge build",
		"clean": "cd contracts && forge clean",
		"extract:abis": "node utils/abis.js",
		"test": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" npx jest",
		"start": "ts-node-dev --respawn --transpile-only operator/index.ts"
	},
	"dependencies": {
		"@reclaimprotocol/js-sdk": "^2.1.1",
		"@reclaimprotocol/zk-fetch": "^0.2.1",
		"alchemy-sdk": "^3.5.6",
		"axios": "^1.8.4",
		"dotenv": "^10.0.0",
		"encoding": "^0.1.13",
		"ethers": "^6.13.2",
		"express": "^4.21.1",
		"openai": "^6.0.1",
		"pinata": "^2.2.0"
	},
	"devDependencies": {
		"@types/axios": "^0.14.4",
		"@types/express": "^5.0.0",
		"@types/jest": "^29.5.13",
		"@types/node": "^20.12.12",
		"@viem/anvil": "^0.0.10",
		"jest": "^29.7.0",
		"ts-jest": "^29.2.5",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.4.5"
	},
	"engines": {
		"node": "20.x"
	}
}
